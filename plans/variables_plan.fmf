summary: Basic variables test
execute:
    how: tmt
    script: |
        final_result=0
        # Test if variable $SOME_DUMMY_VARIABLE is 'FOO_BAR_VARIABLE'
        if [ "$SOME_DUMMY_VARIABLE" != "FOO_BAR_VARIABLE" ]; then
            final_result=1
        fi
        # Test if variable $OS_VARIABLE is 'DUMMY_OS'
        if [ "$OS_VARIABLE" != "DUMMY_OS" ]; then
            final_result=1
        fi
        exit $final_result
